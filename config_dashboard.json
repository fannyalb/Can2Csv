{
  "time_settings": {
    "default_range_hours": 8,
    "auto_extend": true
  },

  "controls": [
    {
      "type": "datetime",
      "id": "input-time-from",
      "label": "Von",
      "default": "now-8h"
    },
    {
      "type": "datetime",
      "id": "input-time-to",
      "label": "Bis",
      "default": "now"
    },
    {
      "type": "button",
      "id": "btn-load",
      "label": "Laden"
    },
    {
      "type": "checkbox",
      "id": "checkbox-live-mode",
      "label": "Fortlaufend (Live-Modus)"
    }
  ],

